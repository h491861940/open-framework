<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.open.framework</groupId>
    <artifactId>open-framework</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <properties>
        <open.modelVersion>1.0-SNAPSHOT</open.modelVersion>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <maven.test.skip>true</maven.test.skip>
        <mybatisplus.version>2.1.6</mybatisplus.version>
        <mybatisplus-spring-boot-starter.version>1.0.5</mybatisplus-spring-boot-starter.version>
        <mysql.version>5.1.44</mysql.version>
        <fastjson.version>1.2.47</fastjson.version>
        <ojdbc.version>11.2.0.3</ojdbc.version>
        <druid.version>1.1.10</druid.version>
        <guava.version>21.0</guava.version>
        <easypoi.version>3.2.0</easypoi.version>
        <poi.version>3.17</poi.version>
    </properties>
    <modules>
        <module>open-all</module>
        <module>open-common</module>
        <module>open-core</module>
        <module>open-dao</module>
        <module>open-cache</module>
        <module>open-msg</module>
        <module>open-file</module>
        <module>open-scheduler</module>
        <module>open-security</module>
        <module>open-supports</module>
        <module>open-demo</module>
        <module>open-study</module>
        <module>open-gateway</module>
    </modules>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.0.3.RELEASE</version>
        <relativePath/>
    </parent>

<dependencyManagement>
