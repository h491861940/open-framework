            <artifactId>open-common</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-core</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-dao</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-file</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-msg</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-scheduler</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId> open-supports</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>
        <dependency>
            <groupId>com.open.framework</groupId>
            <artifactId>open-security</artifactId>
            <version>${open.modelVersion}</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter</artifactId>
